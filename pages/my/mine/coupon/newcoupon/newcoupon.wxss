/* pages/my/mine/coupon/coupon.wxss */

.coupon-list-view {
  padding: 20rpx;
  background: #fff;
  border-radius: 3%;
  /* border-bottom: 30rpx solid #F0F0F0; */
  margin: 30rpx;
}
.coupon-list-view:last-of-type{
  margin-bottom: 360rpx;

}


.coupon-view {
  display: flex;
  align-items: center;
  width: 100%;
  height: 100%;
  border-bottom: 10rpx solid #f0f0f0;
}

.title-text {
  position: relative;
  justify-content: flex-end;
  align-content: center;
  left: 82%;
  font-size: 32rpx;
  color: #333;
  /* margin-top: 10rpx; */
}

.title-desc {
  position: relative;
  right: 17%;
  justify-content: flex-start;
  font-size: 32rpx;
  color: #333;
  /* margin-top: 10rpx; */
}

.no_notice-view {
  display: flex;
  position: absolute;
  height: 100%;
  width: 100%;
  align-items: center;
  justify-content: center;
}

.price-style {
  display: block;
  color: #fff;
  position: absolute;
  top: 60rpx;
  size: 30rpx;
  z-index: 5;
  left: 3%;
  text-align: left;
}

.main-view {
  position: relative;
   display: inline-block;
  min-height: 100vh;
  width: 100%;
  top: 0px;
  bottom: 0px;
  right: 0px;
  left: 0px;
  margin: 0px;
  padding: 0px;
  background: #eee;
}
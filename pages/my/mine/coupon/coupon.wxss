/* pages/my/mine/coupon/coupon.wxss */
.coupon-list-view {
  padding: 20rpx;
  background: #ffffff;
  border-radius: 3%;
  border-bottom: 30rpx solid #F0F0F0;
  margin: 30rpx;
}

.coupon-view {
  display: flex;
  align-items: center;
  width: 100%;
  height: 100%;
  border-bottom: 10rpx solid #F0F0F0;
}

.title-text {
  display: flex;
   justify-content: flex-end;
   bottom: 2rpx;
   align-content:center; 
  font-size: 32rpx;
  color: #333333;
  /* margin-top: 10rpx; */
}

.no_notice-view{
  display: flex; 
  position: absolute;
  height: 100%;
  width: 100%;
  align-items: center;
  justify-content: center;
}
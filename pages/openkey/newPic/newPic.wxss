.air_container {
  width: 100%;
  background-color: white;
  display: flex;
  flex-flow: row wrap;
  align-content: flex-start;
  padding-top: 10rpx;
}

.mv_item {
  position: relative;
  box-sizing: border-box;
  background-color: white;
  flex: 0 0 25%;
  height: 200rpx;
  width: 200rpx;
  margin-top: 10rpx;
  border: 1px solid #F0F0F0;
}

.mv_pic
{
  width: 100%;
  height: 100%;
}

.pic {
  width: 100%;
  height: 100%;
}

.mv_name {
  color: #333333;
  font-weight: 200;
  font-size: 24rpx;
  width: 100%;
  height: 40rpx;
  background: #ccc;
  opacity: .8;
  position: absolute;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 0;
}
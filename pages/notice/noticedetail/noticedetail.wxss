.main-view {
  padding: 20rpx;
}

.title-text {
  display: flex;
  justify-content: center;
  font-size: 32rpx;
  color: #333333;
  margin-top: 10rpx;
}

.item_container-time{
  display: flex;
  height: 30rpx;
  width: 100%;
  align-items: center;
  justify-content: right;
  position: absolute;
  padding: 20rpx;
  color: #999999;
  font-size: 28rpx;
}

.time-text {
  display: flex;
  justify-content: center;
  font-size: 24rpx;
  color: #999999;
  margin-top: 20rpx;
}

.content-text {
  display: flex;
  justify-content: left;
  left: 20rpx;
  right: 10rpx;
  font-size: 30rpx;
  color: #333333;
  margin-top: 20rpx;
}

.order_titile-view{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 80rpx;
  width: 100%;
}

.order_content-view{
  display: flex;
  align-items: center;
  height: 60rpx;
  width: 100%;
}

.content-view {
  padding: 20rpx;
  display: flex;
  justify-content: center;
  color: #333333;
  font-size: 28rpx;
}

.line-view{
  border-bottom: 2rpx solid #F0F0F0;
}

.no_notice-view{
  display: flex; 
  position: absolute;
  height: 100%;
  width: 100%;
  align-items: center;
  justify-content: center;
}
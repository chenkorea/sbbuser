page{  
  background: #f5f5f5;  
}  
/*总体主盒子*/  
.container {  
  position: absolute;  
  width: 100%;  
  height: 100%;  
  background: #ffffff;  
  color: #939393;  
}  
  
/*左侧栏主盒子*/  
.nav_left{  
  /*设置行内块级元素（没使用定位）*/
  position: absolute;  
  top: 0;  
  left: 0;  
  flex: 1;  
  /*宽度75%，高度占满，并使用百分比布局*/  
  display: inline-block;  
  width: 25%;  
  height: 100%;  
  /*主盒子设置背景色为灰色*/  
  background: #f5f5f5;  
  text-align: center;
}  
/*左侧栏list的item*/  
.nav_left .nav_left_items{  
  /*每个高30px*/  
  height: 70rpx;  
  /*垂直居中*/  
  line-height: 60rpx;  
  /*再设上下padding增加高度，总高42px*/  
  padding: 12rpx 0;  
  /*只设下边线*/  
  border-bottom: 1px solid #dedede;  
  /*文字14px*/  
  font-size: 28rpx;  
}  
/*左侧栏list的item被选中时*/  
.nav_left .nav_left_items.active{  
  /*背景色变成白色*/  
  background: #fff;  
}  
  
/*右侧栏主盒子*/  
.nav_right{  
  /*右侧盒子使用了绝对定位*/  
  position: absolute;  
  top: 0;  
  right: 0rpx;  
  flex: 1;  
  /*宽度75%，高度占满，并使用百分比布局*/  
  width: 75%;  
  height: 100%;  
  background: #ffffff;
}  
/*右侧栏list的item*/  
.nav_right .nav_right_items{  
  /*浮动向左*/  
  float: left;  
  /*每个item设置宽度是33.33%*/  
  width: 50%;  
  height: 90rpx;  
  text-align: center; 
  background: #ffffff;
  left: 12rpx
}  
.nav_right .nav_right_items image{  
  /*被图片设置宽高*/  
  width: 50px;  
  height: 30px;  
}  
.nav_right .nav_right_items text{  
  /*给text设成块级元素*/  
  height: 90rpx;
  display: flex;  
  font-size: 24rpx;  
  /*设置文字溢出部分为...*/  
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
  border: 1rpx solid #F0F0F0;
  align-items: center;
  justify-content: center;
}
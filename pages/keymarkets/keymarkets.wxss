.air_container {
  width: 100%;
  background-color: white;
  display: flex;
  flex-flow: row wrap;
  align-content: flex-start;
  padding-top: 10rpx;
}

.mv_item {
  position: relative;
  box-sizing: border-box;
  background-color: white;
  flex: 0 0 25%;
  height: 200rpx;
  width: 200rpx;
  margin-top: 10rpx;
  border: 1px solid #F0F0F0;
}

.mv_pic
{
  width: 100%;
  height: 100%;
}

.pic {
  width: 100%;
  height: 100%;
}

.mv_name {
  color: #8B7355;
  font-weight: 200;
  font-size: 24rpx;
  padding-top: 10rpx;
  padding-left: 10rpx;
}

.mv_price {
  color: #FF0000;
  font-weight: 200;
  font-size: 24rpx;
  padding-top: 10rpx;
  padding-left: 10rpx;
}

.classfy{
  color: #ffffff;
  position: fixed;
  display: flex;
  align-items: center;
  justify-content: center;
  bottom: 20rpx;
  right: 20rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  font-size: 28rpx;
  background: #4EEE94;
}

